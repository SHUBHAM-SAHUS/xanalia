{
  "name": "xanalia-dashboard",
  "version": "3.3.1",
  "description": "xanalia - Admin & Dashboard",
  "author": "",
  "private": true,
  "scripts": {
    "sass:compile": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/assets/scss/ -o public/css/",
    "sass:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "build:css": "npm-run-all sass:*",
    "build:js": "react-scripts build",
    "watch:css": "chokidar \"src/assets/scss/**/*\" -c \"npm run build:css\"",
    "watch:js": "react-scripts start",
    "start": "npm-run-all -p build:css watch:*",
    "build": "npm-run-all -p build:*",
    "format": "prettier --write \"src/**/*.{js,jsx}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "> 0.25%",
    "ie >= 11",
    "not op_mini all",
    "not dead"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-brands-svg-icons": "5.13.0",
    "@fortawesome/free-regular-svg-icons": "5.13.0",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@fortawesome/react-fontawesome": "0.1.9",
    "apexcharts": "3.18.1",
    "availity-reactstrap-validation": "2.6.1",
    "aws-sdk": "^2.947.0",
    "axios": "^0.21.1",
    "bootstrap": "4.4.1",
    "chart.js": "2.9.3",
    "chokidar-cli": "2.1.0",
    "classnames": "2.2.6",
    "dayjs": "^1.10.6",
    "emoji-picker-react": "^3.4.8",
    "fullcalendar": "3.10.1",
    "google-map-react": "1.1.7",
    "ionicons": "4.6.3",
    "jquery": "3.4.1",
    "jvectormap-next": "3.0.0",
    "moment": "^2.24.0",
    "nextjs-websocket": "^1.0.6",
    "node-sass": "^4.13.1",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "4.1.5",
    "postcss-cli": "7.1.0",
    "qrcode.react": "^1.0.1",
    "react": "16.13.1",
